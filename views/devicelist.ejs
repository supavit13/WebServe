  <script>
        var json = '<%- JSON.stringify(device) %>'
        console.log(json);
        json = JSON.parse(json);
        var str="";
        for(var i=0;i<json.length;i++){
            str += "<tr>"+
                    "<td>" + json[i].name + "</td>"+
                    "<td>" + json[i].key + "</td>"+
                    "<td>" + json[i]._id + "</td>"+
                    "<td>" + json[i].discription + "</td>"+
                    "<td>" + json[i].date + "</td>"+
                "</tr>";
        }
        $(".tabody").append(str);
    </script>
  


  </html>