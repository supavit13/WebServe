<% include head.ejs %>

<body>
    <h1>CSV list</h1>
    <button class="btn"><a href="/">Back</a></button>
    <table id="example" class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>Filename</th>
                <th>Link</th>
            </tr>
        </thead>
        <tbody>
            <% for(var i = 0 ; i < data.length ; i++) { %>
            <tr>
                <td><%= data[i] %></td>
                <td><a href="/download/<%= data[i] %>">click for download</a></td>
            </tr>
            <% } %>
        </tbody>
    </table>
</body>

</html>